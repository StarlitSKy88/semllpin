# 生产环境配置
VITE_NODE_ENV=production

# API配置 - 实际的Cloudflare Workers部署地址
VITE_API_URL=https://smellpin-workers.dev-small-1.workers.dev

# PayPal配置 - 生产环境需要使用live环境
VITE_PAYPAL_CLIENT_ID=your-production-paypal-client-id
VITE_PAYPAL_ENVIRONMENT=live

# Google Maps API配置
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# 应用配置
VITE_APP_NAME=SmellPin
VITE_APP_VERSION=1.0.0

# 安全配置
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true