{
  "envId": "your-cloudbase-env-id",
  "framework": {
    "name": "vue",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "buildCommand": "npm run build",
          "outputPath": "dist",
          "cloudPath": "/",
          "ignore": [
            ".git",
            ".gitignore",
            "node_modules",
            ".DS_Store"
          ]
        }
      }
    }
  }
}