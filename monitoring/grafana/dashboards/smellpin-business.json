{
  "dashboard": {
    "id": null,
    "title": "SmellPin 业务指标监控",
    "tags": ["smellpin", "business", "metrics"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "1m",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "用户总数",
        "type": "stat",
        "targets": [
          {
            "expr": "users_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "活跃用户数",
        "type": "stat",
        "targets": [
          {
            "expr": "active_users",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "今日新增用户",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(users_registered_total[24h]))",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "标注总数",
        "type": "stat",
        "targets": [
          {
            "expr": "annotations_total",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "用户注册趋势",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(users_registered_total[1h]))",
            "refId": "A",
            "legendFormat": "每小时注册数"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "标注创建趋势",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(annotations_created_total[1h]))",
            "refId": "A",
            "legendFormat": "每小时创建数"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "LBS奖励发放",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(lbs_rewards_distributed_total[1h]))",
            "refId": "A",
            "legendFormat": "每小时奖励数"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 8,
          "x": 0,
          "y": 17
        }
      },
      {
        "id": 8,
        "title": "支付成功率",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(payments_successful_total[5m])) / sum(rate(payments_total[5m])) * 100",
            "refId": "A",
            "legendFormat": "支付成功率 (%)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {
          "h": 9,
          "w": 8,
          "x": 8,
          "y": 17
        }
      },
      {
        "id": 9,
        "title": "收入统计",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(increase(revenue_total[1h]))",
            "refId": "A",
            "legendFormat": "每小时收入"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyCNY"
          }
        },
        "gridPos": {
          "h": 9,
          "w": 8,
          "x": 16,
          "y": 17
        }
      },
      {
        "id": 10,
        "title": "热门地区标注分布",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (region) (annotations_by_region))",
            "refId": "A",
            "format": "table"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 26
        }
      },
      {
        "id": 11,
        "title": "用户活跃度分布",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (activity_level) (users_by_activity)",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 26
        }
      },
      {
        "id": 12,
        "title": "今日关键指标",
        "type": "table",
        "targets": [
          {
            "expr": "sum(increase(users_registered_total[24h]))",
            "refId": "A",
            "legendFormat": "新增用户"
          },
          {
            "expr": "sum(increase(annotations_created_total[24h]))",
            "refId": "B",
            "legendFormat": "新增标注"
          },
          {
            "expr": "sum(increase(lbs_rewards_distributed_total[24h]))",
            "refId": "C",
            "legendFormat": "奖励发放"
          },
          {
            "expr": "sum(increase(revenue_total[24h]))",
            "refId": "D",
            "legendFormat": "今日收入"
          }
        ],
        "transformations": [
          {
            "id": "seriesToColumns",
            "options": {
              "byField": "Time"
            }
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 35
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "region",
          "type": "query",
          "query": "label_values(annotations_by_region, region)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "部署事件",
          "datasource": "Prometheus",
          "expr": "deployment_events",
          "iconColor": "blue"
        }
      ]
    },
    "schemaVersion": 27,
    "version": 1,
    "links": [
      {
        "title": "系统监控",
        "url": "/d/smellpin-overview/smellpin-system-overview",
        "type": "dashboards"
      }
    ]
  }
}